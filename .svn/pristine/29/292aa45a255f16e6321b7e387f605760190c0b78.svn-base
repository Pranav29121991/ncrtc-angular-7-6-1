<div class="container">
    <div class="epeople-registry row">
        <div class="row">
            <div class="form-group  col-lg-3">
        
                <span class="text-white form-text">Case Type</span>
                <input id="typeahead-basic" type="text" class=" form-control" [(ngModel)]="casetypesearch"
                    placeholder="Enter case type" [ngbTypeahead]="search" tabindex="1" />
        
            </div>
            <div class="form-group col-lg-3">
                <span class="text-white form-text ">Case No</span>
                <input type="text" [(ngModel)]="casenosearch" class="  form-control" placeholder="Enter case no" tabindex="1" />
            </div>
            <div class="form-group col-lg-3">
                <span class="text-white form-text ">Case Year</span>
                <input type="text" [(ngModel)]="yearsearch" class="   form-control" placeholder="Enter case year"
                    tabindex="1" />
            </div>
            <div class="form-group col-lg-3">
                <span class="text-white form-text "></span>
                <input type="button" class=" btn mt-4 btn-primary mr-2" value="Search" (click)="findcrnByCaseTypenoyear()"
                    style="width: 90px;" tabindex="1">
            </div>
        </div>
    </div>
    <div class="row">
        <table  class="table table-striped table-hover table-bordered">
            <thead>
                <tr>
                    <th>
                        CINO
                    </th>
                <th>
                    Pet Name
                </th>
                <th>
                    Res Name
                </th>
                    <th>
                        Case No
                    </th>
                    <th>
                        Action
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr *ngFor="let item of data">
                    <td>
                        {{item.cino}}
                    
                    </td>
                    <td>
                        {{item.pet_name}}
                    
                    </td>
                    <td>
                        {{item.res_name}}
                    
                    </td>
                    <td>
                        {{item.casetype.type_name}}/{{item.reg_no}}/{{item.reg_year}}
                        
                    </td>
                    <td>
                        <button class="btn btn-primary" (click)="GotoMigrate(item)">Migrate</button>
                        <i *ngIf="item.lodder" class="fa ml-2 fa-spinner fa-spin"></i>
                    </td>
                </tr>
                <tr *ngIf="data.length==0">
                    <td class="text-center" colspan="5"> Data not found!</td>
                </tr>
            </tbody>
        </table>
    </div>

</div>
