<div class="row mb-4" mb-10 *ngIf="itemTab.allMetadata(['casefile.*.*','casefile.*']).length > 0">
  <div class="col-lg-6 text-center"><b>{{itemTab.firstMetadataValue('casefile.case.typename')}}/{{itemTab.firstMetadataValue('dc.title')}}/{{itemTab.firstMetadataValue('casefile.case.registrationyear')}}</b></div>
  <div class="col-lg-6"><span *ngIf="caseStatus"class="badge " style="    font-size: 94% !important;" [ngClass]="classbager(caseStatus)">{{ caseStatus | translate }}</span>
    <!-- <ds-dso-page-edit-button [pageRoute]="pageRoute" [dso]="itemTab" [tooltipMsg]="'item.page.edit'"></ds-dso-page-edit-button> -->

    <!-- <ds-dso-edit-menu></ds-dso-edit-menu> -->
  </div>
</div>
<div class="row" >
  <div class="pr-1" style="    width: 95%;height: 560px; overflow: scroll;">
      <ngb-accordion #accordion="ngbAccordion" activeIds="custom-panel-1" (panelChange)="beforeChange($event)">
        <ngb-panel id="custom-panel-1">
          <ng-template ngbPanelHeader let-opened="opened">
            <div class="float-left accordion-button custom-header" [class.collapsed]="!opened">
      
              <button ngbPanelToggle class="p-0 btn  container-fluid text-start ps-0">
                <img class="img-css" src="assets/dsquareIcon/order.svg" alt="" />
                <b class="btn-link pline-height header-title ml-3 ">
                  Basic Details
                </b>
              </button>
            </div>
          </ng-template>
          <ng-template ngbPanelContent>
      
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.title') ">
              <div>
                <h6 class="font-weight-bold">File number</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.title')}}"> {{itemTab.firstMetadataValue('dc.title')}}</span>
      
      
            </div>
      
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.subject') ">
              <div>
                <h6 class="font-weight-bold">Subject</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.subject')}}">
                {{itemTab.firstMetadataValue('dc.subject')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.maintainedby') ">
              <div>
                <h6 class="font-weight-bold">Maintained by</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.maintainedby')}}">
                {{itemTab.firstMetadataValue('dc.maintainedby')}}</span>
            </div>
      
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.department') ">
              <div>
                <h6 class="font-weight-bold">Department</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.department')}}">
                {{itemTab.firstMetadataValue('dc.department')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.date.issued') ">
              <div>
                <h6 class="font-weight-bold">Year </h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.date.issued')}}">
                {{itemTab.firstMetadataValue('dc.date.issued') | date:'dd-MM-yyyy'}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.keywords') ">
              <div>
                <h6 class="font-weight-bold">Keywords</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.keywords')}}">
                {{itemTab.firstMetadataValue('dc.keywords')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('dc.remarks') ">
              <div>
                <h6 class="font-weight-bold">Remarks</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('dc.remarks')}}">
                {{itemTab.firstMetadataValue('dc.remarks')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('casefile.case.NameOfHeadQuarter') ">
              <div>
                <h6 class="font-weight-bold">Headquarter Name </h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('casefile.case.NameOfHeadQuarter')}}">
                {{itemTab.firstMetadataValue('casefile.case.NameOfHeadQuarter')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('casefile.case.NameOfTaluka') ">
              <div>
                <h6 class="font-weight-bold">Taluka</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('casefile.case.NameOfTaluka')}}">
                {{itemTab.firstMetadataValue('casefile.case.NameOfTaluka')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('casefile.case.NameOfDistrict') ">
              <div>
                <h6 class="font-weight-bold">District</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('casefile.case.NameOfDistrict')}}">
                {{itemTab.firstMetadataValue('casefile.case.NameOfDistrict')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('casefile.case.NameOfState') ">
              <div>
                <h6 class="font-weight-bold">State</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('casefile.case.NameOfState')}}">
                {{itemTab.firstMetadataValue('casefile.case.NameOfState')}}</span>
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('casefile.judge.name') ">
              <div>
                <h6 class="font-weight-bold">Judge(s)</h6>
              </div>
              <span *ngFor="let judge of itemTab.allMetadataValues('casefile.judge.name'); let last=last;">
                <a target="_blank" href="/browse/JudgeName?value={{judge }}"><span class="text-uppercase text-muted "
                    data-toggle="tooltip" data-placement="top" title="{{judge}}" [innerHTML]="judge"></span></a> <br />
              </span>
      
            </div>
            <div class="mt-2" *ngIf="itemTab.firstMetadataValue('casefile.judge.designation') ">
              <div>
                <h6 class="font-weight-bold">Judge's Designation</h6>
              </div>
              <span class="text-uppercase " data-toggle="tooltip" data-placement="top"
                title="{{itemTab.firstMetadataValue('casefile.judge.designation')}}">
                {{itemTab.firstMetadataValue('casefile.judge.designation')}}</span>
            </div>
            <div class="mt-2">
      
              <ds-related-items [parentItem]="itemTab" [relationType]="'ConnectedCaseOfCaseFile'"
                [label]="'Connected Case(s)'">
              </ds-related-items>
              <ds-related-items [parentItem]="itemTab" [relationType]="'CaseFileOfMainCaseFile'" [label]="'Main Case'">
              </ds-related-items>
              <ds-related-items [parentItem]="itemTab" [relationType]="'subCaseFileOfMainCaseFile'" [label]="'Main Case'">
              </ds-related-items>
      
      
              <ds-related-items [parentItem]="itemTab" [relationType]="'CaseFileOfIACase'" [label]="'Main Case'">
              </ds-related-items>
              <ds-related-items [parentItem]="itemTab" [relationType]="'CaseFileOfCaveatCase'" [label]="'Main Case'">
              </ds-related-items>
              <div class="simple-view-element" *ngIf="IACase.length>0">
                <h5 class="simple-view-element-header">IA Case(s)</h5>
                <div class="simple-view-element-body" *ngFor="let iacase of IACase">
                  <a target="_blank" class="ml-3 " href="handle/{{iacase.handle}}">
                    {{iacase.firstMetadataValue('casefile.case.typename')}}/{{iacase.firstMetadataValue('dc.title')}}/{{iacase.firstMetadataValue('casefile.case.registrationyear')}}
                  </a>
                </div>
      
              </div>
              <!-- <ds-related-items
                    [parentItem]="itemTab"
                    [relationType]="'IACaseOfCaseFile'"
                    [label]="'IA Case(s)'">
                </ds-related-items> -->
              <ds-related-items [parentItem]="itemTab" [relationType]="'CaveatCaseOfCaseFile'" [label]="'Caveat Case(s)'">
              </ds-related-items>
            </div>
      
          </ng-template>
        </ngb-panel>
      
